{
    "name": "airwallex/airwallex-online-payments-gateway",
    "version": "1.23.4",
    "description": "Official Airwallex Plugin.",
    "homepage": "https://www.airwallex.com",
    "license": "GPL-3.0",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7.2",
        "woocommerce/woocommerce-sniffs": "^1.0",
        "brain/monkey": "^2.6",
        "wp-coding-standards/wpcs": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Airwallex\\": "includes",
            "Airwallex\\PayappsPlugin\\CommonLibrary\\": "payapps-plugin-php-common-lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Airwallex\\Tests\\": "tests/phpunit"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "7.0.0"
        }
    },
    "repositories": {
        "payapps": {
            "type": "composer",
            "url": "https://gitlab.awx.im/api/v4/group/payapps/-/packages/composer/packages.json"
        }
    },
    "require": {
        "airwallex/payapps-plugin-php-common-lib": "1.2.*"
    }
}
